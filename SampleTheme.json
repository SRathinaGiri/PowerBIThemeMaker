{
  "name": "Finance BI – Master Visual Palettes (Light) – v3e",
  "dataColors": ["#1F6FFF", "#4BA3FF", "#00A36C", "#7C5CDB", "#FFB000", "#6B7280", "#FFC933", "#E05656"],
  "tableAccent": "#1F6FFF",
  "background": "#F5F7FA",
  "foreground": "#2C3E50",
  "good": "#00A36C",
  "neutral": "#FFB000",
  "bad": "#E05656",
  "visualStyles": {
    "*": {
      "*": {
        "title": [{ "show": false }],
        "background": [{ "show": true, "color": { "solid": { "color": "#F5F7FA" } }, "transparency": 0 }],
        "border": [{ "show": false }],
        "visualHeader": [{ "show": true, "showTooltipButton": true, "showOptionsMenu": false }]
      }
    },

    "tableEx": {
      "*": {
        "grid": [{ "rowPadding": 2, "textSize": 10 }],
        "columnHeaders": [{
          "fontColor": { "solid": { "color": "#273444" } },
          "backColor": { "solid": { "color": "#E8ECF2" } },
          "autoSizeColumnWidth": true,
          "fontFace": "Arial",
          "textSize": 10,
          "alignment": "Left",
          "wordWrap": true
        }],
        "values": [{
          "fontFamily": "Arial",
          "fontSize": 9,
          "fontColorPrimary": { "solid": { "color": "#2C3E50" } },
          "backColorPrimary": { "solid": { "color": "#FFFFFF" } },
          "fontColorSecondary": { "solid": { "color": "#2C3E50" } },
          "backColorSecondary": { "solid": { "color": "#F0F2F5" } },
          "urlIcon": true,
          "wordWrap": true
        }],
        "total": [{
          "totals": true,
          "label": "Total",
          "fontColor": { "solid": { "color": "#FFFFFF" } },
          "backColor": { "solid": { "color": "#1F6FFF" } },
          "applyToHeaders": false,
          "fontFamily": "Arial",
          "fontSize": 10
        }]
      }
    },

    "matrix": {
      "*": {
        "grid": [{ "rowPadding": 2, "textSize": 10 }],
        "columnHeaders": [{
          "fontColor": { "solid": { "color": "#273444" } },
          "backColor": { "solid": { "color": "#E8ECF2" } },
          "fontFace": "Arial",
          "textSize": 10,
          "alignment": "Left",
          "wordWrap": true
        }],
        "rowHeaders": [{
          "fontColor": { "solid": { "color": "#273444" } },
          "backColor": { "solid": { "color": "#EDF1F6" } },
          "stepped": true,
          "steppedLayoutIndentation": 14,
          "showExpandCollapseButtons": true,
          "expandCollapseButtonsColor": { "solid": { "color": "#3B4C63" } },
          "expandCollapseButtonsSize": 12,
          "fontFamily": "Arial",
          "fontSize": 9
        }],
        "values": [{
          "fontFamily": "Arial",
          "fontSize": 9,
          "fontColorPrimary": { "solid": { "color": "#2C3E50" } },
          "backColorPrimary": { "solid": { "color": "#FFFFFF" } },
          "fontColorSecondary": { "solid": { "color": "#2C3E50" } },
          "backColorSecondary": { "solid": { "color": "#F0F2F5" } },
          "bandedRowHeaders": true,
          "urlIcon": true,
          "wordWrap": true
        }],
        "subTotals": [{
          "rowSubTotals": true,
          "columnSubTotals": true,
          "rowSubtotalsLabel": "Total",
          "columnSubtotalsLabel": "Total",
          "fontColor": { "solid": { "color": "#273444" } },
          "backColor": { "solid": { "color": "#E1E6EE" } },
          "applyToHeaders": false,
          "fontFamily": "Arial",
          "fontSize": 10,
          "rowSubTotalPosition": "Bottom"
        }],
        "rowTotal": [{
          "fontColor": { "solid": { "color": "#FFFFFF" } },
          "backColor": { "solid": { "color": "#1F6FFF" } },
          "applyToHeaders": false,
          "fontFamily": "Arial",
          "fontSize": 11
        }],
        "columnTotal": [{
          "fontColor": { "solid": { "color": "#FFFFFF" } },
          "backColor": { "solid": { "color": "#1F6FFF" } },
          "applyToHeaders": false,
          "fontFamily": "Arial",
          "fontSize": 11
        }],
        "total": [{
          "totals": true,
          "fontColor": { "solid": { "color": "#FFFFFF" } },
          "backColor": { "solid": { "color": "#1F6FFF" } },
          "applyToHeaders": false,
          "fontFamily": "Arial",
          "fontSize": 11
        }]
      }
    },

    "slicer": {
      "*": {
        "header": [{
          "show": true,
          "fontColor": { "solid": { "color": "#2C3E50" } },
          "background": { "solid": { "color": "#E8ECF2" } },
          "outline": "None",
          "textSize": 11,
          "fontFamily": "Arial"
        }],
        "items": [{
          "fontColor": { "solid": { "color": "#2C3E50" } },
          "backgroundColor": { "solid": { "color": "#FFFFFF" } },
          "hoverColor": { "solid": { "color": "#F2F5FA" } },
          "selectedColor": { "solid": { "color": "#DDE7FF" } },
          "selectedTextColor": { "solid": { "color": "#1B3C95" } },
          "outline": "None",
          "textSize": 10,
          "fontFamily": "Arial"
        }],
        "numericInputStyle": [{
          "fontColor": { "solid": { "color": "#2C3E50" } },
          "background": { "solid": { "color": "#FFFFFF" } },
          "outline": "None",
          "textSize": 10,
          "fontFamily": "Arial"
        }],
        "date": [{
          "fontColor": { "solid": { "color": "#2C3E50" } },
          "background": { "solid": { "color": "#FFFFFF" } },
          "textSize": 10,
          "fontFamily": "Arial"
        }],
        "slider": [{ "show": true, "background": { "solid": { "color": "#E2E8F0" } } }],
        "background": [{ "show": true, "color": { "solid": { "color": "#F5F7FA" } }, "transparency": 0 }],
        "general": [{ "outlineColor": { "solid": { "color": "#1F6FFF" } }, "outlineWeight": 1, "responsive": false }]
      }
    },

    "advancedSlicerVisual": {
      "*": {
        "background": [{ "show": true, "color": { "solid": { "color": "#F5F7FA" } }, "transparency": 0 }],
        "fillCustom": [{ "$id": "default", "fillColor": { "solid": { "color": "#FFFFFF" } } }],
        "value": [{ "$id": "default", "textWrap": true, "color": { "solid": { "color": "#2C3E50" } } }],
        "outline": [{ "$id": "default", "lineColor": { "solid": { "color": "#1F6FFF" } }, "transparency": 20 }],
        "states": [{ "selectedColor": { "solid": { "color": "#DDE7FF" } }, "hoverColor": { "solid": { "color": "#F2F5FA" } } }]
      }
    },

    "barChart": {
      "*": {
        "legend": [{ "show": true, "position": "Top" }],
        "labels": [{ "show": true, "labelPosition": "InsideEnd", "fontSize": 9, "fontFamily": "Arial", "color": { "solid": { "color": "#273444" } } }],
        "categoryAxis": [{
          "show": true,
          "labelColor": { "solid": { "color": "#2C3E50" } },
          "titleColor": { "solid": { "color": "#4B5563" } },
          "fontSize": 9, "fontFamily": "Arial",
          "gridLineShow": true, "gridLineColor": { "solid": { "color": "#E5EAF0" } }, "gridLineThickness": 1
        }],
        "valueAxis": [{
          "show": true,
          "labelColor": { "solid": { "color": "#2C3E50" } },
          "titleColor": { "solid": { "color": "#4B5563" } },
          "fontSize": 9, "fontFamily": "Arial",
          "gridLineShow": true, "gridLineColor": { "solid": { "color": "#E5EAF0" } }, "gridLineThickness": 1
        }],
        "series": [{ "colors": ["#1F6FFF", "#4BA3FF", "#2F79C7", "#7DA8FF", "#A7C2FF"] }]
      }
    },

    "clusteredBarChart": {
      "*": {
        "legend": [{ "show": true, "position": "Top" }],
        "labels": [{ "show": true, "labelPosition": "InsideEnd", "fontSize": 9, "fontFamily": "Arial", "color": { "solid": { "color": "#273444" } } }],
        "categoryAxis": [{
          "show": true,
          "labelColor": { "solid": { "color": "#2C3E50" } },
          "titleColor": { "solid": { "color": "#4B5563" } },
          "fontSize": 9, "fontFamily": "Arial",
          "gridLineShow": true, "gridLineColor": { "solid": { "color": "#E5EAF0" } }, "gridLineThickness": 1
        }],
        "valueAxis": [{
          "show": true,
          "labelColor": { "solid": { "color": "#2C3E50" } },
          "titleColor": { "solid": { "color": "#4B5563" } },
          "fontSize": 9, "fontFamily": "Arial",
          "gridLineShow": true, "gridLineColor": { "solid": { "color": "#E5EAF0" } }, "gridLineThickness": 1
        }],
        "series": [{ "colors": ["#1F6FFF", "#4BA3FF", "#2F79C7", "#7DA8FF", "#A7C2FF"] }]
      }
    },

    "columnChart": {
      "*": {
        "legend": [{ "show": true, "position": "Top" }],
        "labels": [{ "show": true, "labelPosition": "InsideEnd", "fontSize": 9, "fontFamily": "Arial", "color": { "solid": { "color": "#273444" } } }],
        "categoryAxis": [{
          "show": true,
          "labelColor": { "solid": { "color": "#2C3E50" } },
          "titleColor": { "solid": { "color": "#4B5563" } },
          "fontSize": 9, "fontFamily": "Arial",
          "gridLineShow": true, "gridLineColor": { "solid": { "color": "#E5EAF0" } }, "gridLineThickness": 1
        }],
        "valueAxis": [{
          "show": true,
          "labelColor": { "solid": { "color": "#2C3E50" } },
          "titleColor": { "solid": { "color": "#4B5563" } },
          "fontSize": 9, "fontFamily": "Arial",
          "gridLineShow": true, "gridLineColor": { "solid": { "color": "#E5EAF0" } }, "gridLineThickness": 1
        }],
        "series": [{ "colors": ["#1F6FFF", "#4BA3FF", "#2F79C7", "#7DA8FF", "#A7C2FF"] }]
      }
    },

    "clusteredColumnChart": {
      "*": {
        "legend": [{ "show": true, "position": "Top" }],
        "labels": [{ "show": true, "labelPosition": "InsideEnd", "fontSize": 9, "fontFamily": "Arial", "color": { "solid": { "color": "#273444" } } }],
        "categoryAxis": [{
          "show": true,
          "labelColor": { "solid": { "color": "#2C3E50" } },
          "titleColor": { "solid": { "color": "#4B5563" } },
          "fontSize": 9, "fontFamily": "Arial",
          "gridLineShow": true, "gridLineColor": { "solid": { "color": "#E5EAF0" } }, "gridLineThickness": 1
        }],
        "valueAxis": [{
          "show": true,
          "labelColor": { "solid": { "color": "#2C3E50" } },
          "titleColor": { "solid": { "color": "#4B5563" } },
          "fontSize": 9, "fontFamily": "Arial",
          "gridLineShow": true, "gridLineColor": { "solid": { "color": "#E5EAF0" } }, "gridLineThickness": 1
        }],
        "series": [{ "colors": ["#1F6FFF", "#4BA3FF", "#2F79C7", "#7DA8FF", "#A7C2FF"] }]
      }
    },

    "lineChart": {
      "*": {
        "legend": [{ "show": true, "position": "Top" }],
        "dataPoint": [{ "showAllDataPoints": true }],
        "lineStyles": [{ "strokeWidth": 2, "showMarker": true, "markerSize": 4 }],
        "categoryAxis": [{
          "show": true,
          "labelColor": { "solid": { "color": "#2C3E50" } },
          "titleColor": { "solid": { "color": "#4B5563" } },
          "fontSize": 9, "fontFamily": "Arial",
          "gridLineShow": true, "gridLineColor": { "solid": { "color": "#E5EAF0" } }, "gridLineThickness": 1
        }],
        "valueAxis": [{
          "show": true,
          "labelColor": { "solid": { "color": "#2C3E50" } },
          "titleColor": { "solid": { "color": "#4B5563" } },
          "fontSize": 9, "fontFamily": "Arial",
          "gridLineShow": true, "gridLineColor": { "solid": { "color": "#E5EAF0" } }, "gridLineThickness": 1
        }],
        "series": [{ "colors": ["#2C3E50", "#6B7280", "#94A3B8", "#475569", "#334155"] }]
      }
    },

    "areaChart": {
      "*": {
        "legend": [{ "show": true, "position": "Top" }],
        "dataPoint": [{ "showAllDataPoints": true }],
        "lineStyles": [{ "strokeWidth": 2, "showMarker": false }],
        "categoryAxis": [{
          "show": true,
          "labelColor": { "solid": { "color": "#2C3E50" } },
          "titleColor": { "solid": { "color": "#4B5563" } },
          "fontSize": 9, "fontFamily": "Arial",
          "gridLineShow": true, "gridLineColor": { "solid": { "color": "#E5EAF0" } }, "gridLineThickness": 1
        }],
        "valueAxis": [{
          "show": true,
          "labelColor": { "solid": { "color": "#2C3E50" } },
          "titleColor": { "solid": { "color": "#4B5563" } },
          "fontSize": 9, "fontFamily": "Arial",
          "gridLineShow": true, "gridLineColor": { "solid": { "color": "#E5EAF0" } }, "gridLineThickness": 1
        }],
        "series": [{ "colors": ["#7DA8FF", "#A7C2FF", "#C9D8FF", "#E6EFFF"] }]
      }
    },

    "pieChart": {
      "*": {
        "labels": [{
          "show": true,
          "labelStyle": "Category, data value, percent of total",
          "fontSize": 9,
          "fontFamily": "Arial",
          "color": { "solid": { "color": "#2C3E50" } }
        }]
      }
    },

    "donutChart": {
      "*": {
        "labels": [{
          "show": true,
          "labelStyle": "Category, data value, percent of total",
          "fontSize": 9,
          "fontFamily": "Arial",
          "color": { "solid": { "color": "#2C3E50" } }
        }],
        "slices": [{ "innerRadiusRatio": 35 }]
      }
    }
  }
}